  		</div>
  	</div>
<div class="container" style="margin-top:80px">
<!-- FIRST ROW OF BLOCKS -->     
  	<div class="row">
  		<div class="col-sm-12 col-lg-12">
			<h1 style="color:#e2e2e2">Search {{label}}</h1>
			<table class="table-striped" style="background:#fff" cellpadding="8" width="100%">
				<tr>
					<th>IP</th><th>Customer Name</th><th>HNI</th><th>DoV</th><th>Predicted SKU</th><th>Desc</th><th>Confidence</th>
					<th>Suggested Recommendation</th>
					<th>Action Taken Date </th>
					<th>Actual SKU</th>

				</tr>
				{{#data}}
				<tr>
				
					<td>{{ip}}</td>
					<td>{{customerName}}</td>
					<td>{{hni}}</td>
					<td>{{dov}}</td>
					<td>{{predictedSKU}}</td>
					<td>{{desc}}</td>
					<td>{{confidence}}</td>
					<td>{{suggestedRecom}}</td>
					<td>{{actionTakenDate}}</td>
					<td>{{actualSKU}}</td>
				</tr>
				{{/data}}
			</table>
		</div>
		</div>
	</div>

  </body>
